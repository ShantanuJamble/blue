<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
  <folder name="Editors">
    
    <folder name="FontsColors">
      <folder name="blueDefault">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="blue.ui.editor.Bundle"/>
        <attr name="position" intvalue="0"/>
        <folder name="Defaults">
          <file name="org-netbeans-modules-defaults-token-colorings.xml" url="blueTokenColorings.xml">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="blue.ui.editor.Bundle"/>
          </file>
          <file name="org-netbeans-modules-defaults-highlight-colorings.xml" url="blueHighlightColorings.xml">
            <attr name="SystemFileSystem.localizingBundle" stringvalue="blue.ui.editor.Bundle"/>
            <attr name="nbeditor-settings-ColoringType" stringvalue="highlight"/>
          </file>
        </folder>
      </folder>
    </folder>
    
    <folder name="text">
      
      <!-- Csound ORC -->
      
      <folder name="x-csound-orc">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="blue.ui.editor.csound.orc.Bundle"/>
        <file name="language.instance">
          <attr name="instanceCreate" methodvalue="blue.ui.editor.csound.orc.CsoundOrcTokenId.language"/>
          <attr name="instanceOf" stringvalue="org.netbeans.api.lexer.Language"/>
        </file>
        <file name="EditorKit.instance">
          <attr name="instanceClass" stringvalue="blue.ui.editor.csound.orc.CsoundOrcEditorKit"/>
          <attr name="instanceOf" stringvalue="javax.swing.text.EditorKit,blue.ui.editor.csound.orc.CsoundOrcEditorKit"/>
          <attr name="beaninfo" boolvalue="false"/>
        </file>
        <!--
        <folder name="TokenListProvider">
          <file name="org-netbeans-modules-spellchecker-plain-PlainTokenListProvider.instance" />
        </folder>-->
        
        <folder name="FontsColors">
          <folder name="blueDefault">
            <folder name="Defaults">
              <file name="FontAndColors.xml" url="csound/orc/FontAndColors.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="blue.ui.editor.csound.orc.Bundle"/>
              </file>
            </folder>
          </folder>
        </folder>
        
      </folder>
      
      <!-- BSB -->
      
      <folder name="x-blue-synth-builder">
        <attr name="SystemFileSystem.localizingBundle" stringvalue="blue.ui.editor.csound.orc.Bundle"/>
        <file name="language.instance">
          <attr name="instanceCreate" methodvalue="blue.ui.editor.csound.orc.CsoundOrcTokenId.language"/>
          <attr name="instanceOf" stringvalue="org.netbeans.api.lexer.Language"/>
        </file>
        <file name="EditorKit.instance">
          <attr name="instanceClass" stringvalue="blue.ui.editor.blueSynthBuilder.BlueSynthBuilderEditorKit"/>
          <attr name="instanceOf" stringvalue="javax.swing.text.EditorKit,blue.ui.editor.blueSynthBuilder.BlueSynthBuilderEditorKit"/>
          <attr name="beaninfo" boolvalue="false"/>
        </file>
        <!--
        <folder name="TokenListProvider">
          <file name="org-netbeans-modules-spellchecker-plain-PlainTokenListProvider.instance" />
        </folder>-->
        
        <folder name="FontsColors">
          <folder name="blueDefault">
            <folder name="Defaults">
              <file name="FontAndColors.xml" url="csound/orc/FontAndColors.xml">
                <attr name="SystemFileSystem.localizingBundle" stringvalue="blue.ui.editor.csound.orc.Bundle"/>
              </file>
            </folder>
          </folder>
        </folder>
      </folder>
      
      
    </folder>
  </folder>
  
  <folder name="OptionsDialog">
    <folder name="PreviewExamples">
      <folder name="text">
        <file name="x-csound-orc" url="csound/orc/CsoundOrcTemplate.orc"/>
      </folder>
    </folder>
  </folder>
</filesystem>

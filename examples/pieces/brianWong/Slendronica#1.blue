      <instrument type='blue.orchestra.BlueSynthBuilder' editEnabled='true'>
        <name>Modist-00</name>
        <comment>Modist - A basic score-controlled ModFM synth based on the Victor Lazzarini&apos;s design, as presented in Csound Journal # 11. by Brian Wong 2009</comment>
        <globalOrc>gi_sine_&lt;INSTR_ID&gt;	ftgen 0, 0, 16384, 10, 1
</globalOrc>
        <globalSco>f100 0 131072 &quot;exp&quot; 0 -50 1</globalSco>
        <instrumentText>ifq = p4				; base frequency

icfa = p5				; Carrier Factor envelope
icfb = p6
icfc = p7
icft = p8
icfu = p9
icfv = p10
kfc transeg icfa, icft, icfu, icfb, (p3-icft), icfv, icfc
kfc = ifq * kfc

inpa = p11				; Amplitude envelope
inta = p12
inxa = p13
inpb = p14
intb = p15
inxb = p16
inpc = p17
inxc = p18
inpd = p19
intd = p20
inxd = p21
kamp transeg inpa,inta,inxa,inpb,intb,inxb,inpc,p3-inta-intb-intd,inxc,inpd,intd,inxd,0
kamp = kamp * 10000

imfa = p22				; Modulator Factor envelope
imfb = p23
imfc = p24
imft = p25
imfu = p26
imfv = p27
kfm transeg imfa, imft, imfu, imfb, (p3-imft), imfv, imfc
kfm = ifq * kfm

imipa = p28				; Modulation Index envelope
imita = p29
imixa = p30
imipb = p31
imitb = p32
imixb = p33
imipc = p34
imixc = p35
imipd = p36
imitd = p37
imixd = p38
kndx transeg imipa,imita,imixa,imipb,imitb,imixb,imipc,p3-imita-imitb-imitd,imixc,imipd,imitd,imixd,0

imx = p39				; Maximum value for exponential funtion table

isin = gi_sine_&lt;INSTR_ID&gt;		; sine
iexp = 100				; exponential function table : does not work with ftgen, can only exist as numbered table in global score

acar oscili kamp,kfc,isin,0.25		; ModFM algorithm
acos oscili 1,kfm,isin,0.25
amod table  -kndx*(acos-1)/imx,iexp,1
aout = acar*amod

kpan line p40, p3, p41			; panning
aleft = kpan * aout
aright = (1-kpan) * aout

blueMixerOut aleft, aright
</instrumentText>

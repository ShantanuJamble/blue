  <arrangement>
    <instrumentAssignment arrangementId='1' isEnabled='true'>
      <instrument type='blue.orchestra.BlueSynthBuilder' editEnabled='true'>
        <name>Voice</name>
        <comment></comment>
        <globalOrc/>
        <globalSco/>
        <instrumentText>ifreq = p4
iamp = ampdbfs(p5)
irvb = ampdbfs(-12)
  
;; Signal Generator
kfreq = ifreq * (lfo(&lt;lfo_amp&gt;, &lt;lfo_freq&gt;) + 1)
asig = vco2(iamp, kfreq)
asig += vco2(iamp, kfreq * 2, 12)

  
;; Signal Processing
  
asig = zdf_ladder(asig, &lt;filt_cut&gt;, 4)
;asig = zdf_ladder(asig, expon:a(sr / 3, p3, ifreq), 4)

asig *= linsegr(0, 0.25, 1, 0.25, 0) * ampdbfs(&lt;gain_adj&gt;)
  
;; Panning and send to mixer
al, ar pan2 asig, &lt;pan&gt;
 
;sbus_mix(0, al, ar)
;sbus_mix(1, al * irvb, ar * irvb)

blueMixerOut &quot;&lt;subchan_send&gt;&quot;, al * &lt;reverb_amt&gt;, ar * &lt;reverb_amt&gt;

blueMixerOut al, ar
</instrumentText>

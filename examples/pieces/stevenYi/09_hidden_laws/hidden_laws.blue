(defn bit-proc [indx bit-mask-val bit-to-check]
    (bit-and (bit-shift-right indx (bit-and indx bit-mask-val)) bit-to-check)
)

(defn bit-score [instr len bit-pattern bit-to-check pch]
    (gen-score
        instr
        (reduce
            #(let [bit (bit-proc %2 bit-pattern bit-to-check)]
                (if (= bit bit-to-check)
                (conj % (+ %2 (rand 0.01)))
                %))
            []
            (range len))
        0.95
        (hertz pch)
        -12))

</clojureCode>

# ADDITIONAL CLASS METHODS FOR PERFORMER GROUP

def performSplit(self, notes):
	retVal = &quot;&quot;
	
	counter = 0

	for i in range(len(notes)):	
		retVal += self[counter].perform(NoteList( [notes[i]] ) )
		counter += 1
		if counter &gt;= len(self):
			counter = 0

	return retVal


PerformerGroup.performSplit = performSplit



def createChord(startPch, intervalArray, amp):
	nl = NoteList()
	pch = startPch

	nl.append(  Note(0, 1, pch, pch, amp, 1) )
	for interval in intervalArray:
		pch = pchAdd(pch, interval)
		nl.append( Note(0, 1, pch, pch, amp, 1) )
	return nl

def simpleNote(pch, amp):
	return NoteList([ Note(0, 1, pch, pch, amp, 1) ])




    <instrumentAssignment arrangementId='1' isEnabled='true'>
      <instrument type='blue.orchestra.GenericInstrument'>
        <name>chebyshev2</name>
        <comment></comment>
        <globalOrc>; simple percussive envelope (from GEN05 man page)
giEnv	ftgen	10, 0, 64, 5, 1, 2, 120, 60, 1, 1, 0.001, 1

</globalOrc>
        <globalSco></globalSco>
        <instrumentText>idur	=	p3		; duration
iamp	=	ampdb(p4)	; amplitude
ipitch	=	p5		; center frequency (Hz)
ipan	=	p6		; pan
itab	=	p7		; table for oscillator
ibeat	=	p8		;

; LFOs to drive harmonics
k1	_lfo	 1, ibeat, 0
k2	_lfo	 2, ibeat, 0.1
k3	_lfo	 3, ibeat, 0.2
k5	_lfo	 5, ibeat, 0.3
;k7	_lfo	 7, ibeat
;k11	_lfo	11, ibeat
;k13	_lfo	13, ibeat
;k17	_lfo	17, ibeat

; generate wave
kenv	linen	1, 0, idur, 1 
ax	poscil3	kenv, ipitch, itab

; waveshape it
az	chebyshevpoly	ax, 0, k1, k2, k3, 0, k5;, 0, k7, 0, 0, 0, k11;, 0, k13;, 0, 0, 0, k17
aout	dcblock2	az

; output
al,ar	pan_equal_power	aout*iamp, ipan
blueMixerOut	al, ar
</instrumentText>

        <noteProcessorChain/>
        <pythonCode>instr = 1
amp = -12
table = 1	# sine

def play(ratio, reduce, base, pan):
 	return &quot;i%d 0 1 %f %f %f %d %f\n&quot; % (instr, amp, base*ratio, pan, table, ratio/reduce)

def play_series(series, reduce=7, base=240, pan=0.0):
	out = &quot;&quot;
	for ratio in series:
		out += play(ratio, reduce, base, pan)
	return out</pythonCode>
